package ch06;

class Person {
	String name = "È«±æµ¿";
	int age = 15;
	String job = "ÇÐ»ý";
	
	@Override
	public String toString() {
		return super.toString();
	}
}

public class ObjectEx02 {

	public static void main(String[] args) {
		Integer num = 10;
		String s = num.toString();
		System.out.println(s);
		
		Person p = new Person();
